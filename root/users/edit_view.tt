[% META title = 'Add User' -%]
<form method="post" action="[% c.uri_for('/')%]id/[% rec.id %]/edit">
<table>
<tr><td>Username:</td><td><input type="text" name="username" value=[% rec.username %]></td></tr>
<tr><td>Email:</td><td><input type="text" name="email" value=[% rec.email %]></td></tr>
<tr><td>Password:</td><td><input type="password" name="password"></td></tr>
</table>
<input type="submit" name="Submit" value="Submit">
</form>